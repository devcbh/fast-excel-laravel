{
    "name": "devcbh/fast-excel-laravel",
    "description": "Lightweight and very fast XLSX Excel Spreadsheet Export/Import for Laravel",
    "keywords": [
        "excel",
        "xlsx",
        "PHPExcel",
        "spreadsheet",
        "laravel",
        "export",
        "import",
        "ms office",
        "office 2007"
    ],
    "type": "library",
    "homepage": "https://github.com/devcbh/fast-excel-laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "devcbh\\FastExcelLaravel\\": "./src/FastExcelLaravel"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "devcbh\\FastExcelLaravel\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "avadim/fast-excel-writer": "^6.4",
        "avadim/fast-excel-reader": "^2.20",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "devcbh\\FastExcelLaravel\\Providers\\ExcelServiceProvider"
            ],
            "aliases": {
                "Excel": "devcbh\\FastExcelLaravel\\Facades\\Excel"
            }
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
